@import '../Variables'

.mystack
  .rotated
    .title
      min-width: calc(100vw - 110px)
      max-width: 280px
      text-align: start

  &__list-container
    width: calc(100vw - 110px)

  &__list
    margin: 0
    padding: 0
    font-size: 18px

    &-item
      margin-bottom: 5px
      list-style none

      &--image
        height: 20px
        margin-right: 10px
